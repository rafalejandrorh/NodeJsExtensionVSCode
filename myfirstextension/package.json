{
  "name": "myfirstextension",
  "displayName": "myFirstExtension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "myfirstextension.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "myfirstextension.goodBye",
        "title": "GoodBye"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-explorer",
          "title": "Package Explorer",
          "icon": "resources/home-2502.svg"
        }
      ]
    },
    "views": {
      "package-explorer": [
        {
          "id": "package-dependencies",
          "name": "Dependencies"
        },
        {
          "id": "package-outline",
          "name": "Outline"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "package-dependencies",
        "contents": "Bienvenido a mi extensi√≥n.\n [Say Hello](command:myfirstextension.helloWorld)\n [Say GoodBye](command:myfirstextension.goodBye)",
        "when": "config.git.enabled && workbenchState == empty"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
